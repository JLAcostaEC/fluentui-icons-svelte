{
	"name": "fluentui-icons-svelte",
	"version": "1.0.0",
	"description": "FluentUI Icons for Svelte 5",
	"repository": {
		"type": "git",
		"url": "https://github.com/JLAcostaEC/fluentui-icons-svelte"
	},
	"author": {
		"name": "Jorge Acosta",
		"url": "https://github.com/JLAcostaEC"
	},
	"license": "MIT",
	"keywords": [
		"fluentui",
		"icons",
		"svelte",
		"svelte-5"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm generate && vite build && pnpm prepare",
		"build:package": "pnpm generate && vite build && pnpm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"generate:package": "svgtosvelte node_modules/@fluentui/svg-icons/icons -t -r -k -f 12 16 20 28 32 48 filled regular -e 24",
		"generate:static": "svgtosvelte src/internal/icons -t -r -k -e 24",
		"generate": "pnpm generate:package && pnpm generate:static",
		"changeset:version": "changeset version && git add --all",
		"changeset:publish": "changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*.svelte": {
			"types": "./dist/icon/*.svelte.d.ts",
			"import": "./dist/icon/*.svelte"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@eslint/compat": "^1.2.6",
		"@eslint/js": "^9.20.0",
		"@fluentui/svg-icons": "^1.1.274",
		"@jlacostaec/svgtosvelte": "^1.1.6",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.0.7",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.0.0",
		"prettier": "^3.5.1",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.6",
		"svelte": "^5.20.2",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.7",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.1.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"overrides": {}
	},
	"packageManager": "pnpm@10.2.0"
}
